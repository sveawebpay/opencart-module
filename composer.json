{
    "name": "sveawebpay/opencart-module",
    "description": "Payment extension for OpenCart",
    "authors": [
        {
            "name": "Svea Ekonomi AB",
            "email": "support-webpay@sveaekonomi.se"
        },
        {
            "name": "FooMedia AB",
            "email": "support@foomedia.se"
        }
    ],
    "require": {
        "sveaekonomi/checkout": "^1.3.1",
        "sveaekonomi/webpay": "^3.13.1"
    },
    "config": {
        "vendor-dir": "src/svea/"
    },
    "autoload": {
        "psr-4": {
            "Svea\\WebPay\\": "sveaekonomi/webpay/src",
            "Svea\\Checkout\\": "sveaekonomi/checkout/src"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable" : true
}
